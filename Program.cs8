using System;

class Program
{
    static void Main()
    {
        int[] counts = new int[6];
        Random rnd = new Random();

        for (int i = 0; i < 100000; i++)
            counts[rnd.Next(6)]++;

        for (int i = 0; i < 6; i++)
            Console.WriteLine($"{i + 1}: {(counts[i] / 1000.0):F2}%");
    }
}





